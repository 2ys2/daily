		int N = Integer.parseInt(br.readLine());
		
		int[] arr = new int[];
		arr[0] = 1;
		for (int i = 1; i < ; i++ ) {
			arr[i] += arr[i-1] + 6*i; 
			
			if ( a[i-1]< N <=a[i]) {
				System.out.println( (a[i] - a[i-1] )/6 + 1 );
			}
		}
		
	}
}
